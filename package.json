{
  "name": "terralanes-backend",
  "version": "1.0.0",
  "description": "Terralanes API",
  "main": "handler.js",
  "scripts": {
    "db:start": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all && node data/addLedger.js",
    "db:stop": "npx sequelize-cli db:drop",
    "db:restart": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all && node data/addLedger.js",
    "elastic:start": "node elastic/elastic.js",
    "elastic:stop": "node elastic/deleteElastic.js",
    "elastic:index": "node elastic/indices.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "faker": "^5.1.0",
    "jest": "^26.4.2",
    "sequelize-cli": "^6.2.0",
    "sequelize-mock": "^0.10.2"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.10.0",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "inquirer": "^7.3.3",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.3.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-auto": "^0.5.3",
    "serverless": "^2.18.0",
    "serverless-offline": "^6.8.0",
    "serverless-dependson-plugin": "^1.1.2",
    "states-abbreviations": "^1.0.0",
    "string-similarity": "^4.0.3"
  }
}
